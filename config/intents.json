{
  "click_first_article": {
    "description": "Click the first prominent article on a news website",
    "primary_selector": "article h2 a, article h3 a, .story-body__h1, .gs-c-promo-heading__title",
    "fallbacks": [
      {
        "method": "css",
        "value": "[data-testid='card-headline'] a"
      },
      {
        "method": "css", 
        "value": ".media__link"
      },
      {
        "method": "css",
        "value": ".story-link"
      },
      {
        "method": "css",
        "value": "h2 a"
      },
      {
        "method": "css",
        "value": "h3 a"
      },
      {
        "method": "text-contains",
        "value": "Read more"
      },
      {
        "method": "text-contains", 
        "value": "Full story"
      }
    ]
  },
  "click_next": {
    "description": "Click next page or continue button",
    "primary_selector": "Next",
    "fallbacks": [
      {
        "method": "text-contains",
        "value": "Next"
      },
      {
        "method": "text-contains",
        "value": "Continue"
      },
      {
        "method": "text-contains",
        "value": "More"
      },
      {
        "method": "aria-label",
        "value": "Next"
      },
      {
        "method": "css",
        "value": ".next-button"
      },
      {
        "method": "css",
        "value": "[aria-label*='next']"
      }
    ]
  },
  "find_top_story": {
    "description": "Find and click the top story on news sites",
    "primary_selector": ".top-story a, .lead-story a",
    "fallbacks": [
      {
        "method": "css",
        "value": ".storylink"
      },
      {
        "method": "css",
        "value": ".titleline > a"
      },
      {
        "method": "css",
        "value": "tr.athing:first-child .titleline a"
      },
      {
        "method": "css",
        "value": ".post-title a"
      }
    ]
  },
  "search_submit": {
    "description": "Submit a search form",
    "primary_selector": "[type='submit']",
    "fallbacks": [
      {
        "method": "text-contains",
        "value": "Search"
      },
      {
        "method": "css",
        "value": "button[type='submit']"
      },
      {
        "method": "css",
        "value": ".search-button"
      },
      {
        "method": "css",
        "value": "[value='Search']"
      }
    ]
  },
  "click_login": {
    "description": "Click login or sign in button",
    "primary_selector": "Sign in",
    "fallbacks": [
      {
        "method": "text-contains",
        "value": "Login"
      },
      {
        "method": "text-contains",
        "value": "Sign in"
      },
      {
        "method": "text-contains",
        "value": "Log in"
      },
      {
        "method": "css",
        "value": ".login-button"
      },
      {
        "method": "css",
        "value": "[data-testid='login']"
      }
    ]
  }
}