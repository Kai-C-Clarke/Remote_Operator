{
  "click_first_article": {
    "primary_selector": "article h2 a, h2 a, .story-link a",
    "timeout": 3000,
    "fallbacks": [
      {
        "method": "css",
        "value": "[data-testid='card-headline'] a"
      },
      {
        "method": "css",
        "value": ".media__link"
      },
      {
        "method": "css",
        "value": ".post-title a"
      },
      {
        "method": "css",
        "value": "h3 a"
      },
      {
        "method": "xpath",
        "value": "//a[contains(@class, 'storylink')]"
      }
    ]
  },
  "click_top_story": {
    "primary_selector": ".top-story a, .lead-story a, .featured a",
    "timeout": 3000,
    "fallbacks": [
      {
        "method": "css",
        "value": ".headline a"
      },
      {
        "method": "css",
        "value": ".main-story a"
      },
      {
        "method": "css",
        "value": "h1 a"
      }
    ]
  },
  "click_latest_news": {
    "primary_selector": ".latest a, .breaking a, .news-item a",
    "timeout": 3000,
    "fallbacks": [
      {
        "method": "css",
        "value": ".story a"
      },
      {
        "method": "css",
        "value": ".article-link"
      },
      {
        "method": "css",
        "value": "article a"
      }
    ]
  }
}